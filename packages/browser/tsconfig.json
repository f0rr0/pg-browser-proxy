{
  "extends": ["@pg-browser-proxy/tsconfig/base.json"],
  "compilerOptions": {
    "lib": ["DOM", "ES2020"],
    "jsx": "react-jsx",
    "target": "ES2020",
    "module": "ESNext",
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "strict": true
  },
  "include": ["./**/*.ts", "./**/*.tsx"]
}
